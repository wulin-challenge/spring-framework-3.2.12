<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:spring="http://www.springframework.org/schema/beans"
		xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
       
 	
	<bean id="myBeanPostProcessor" class="cn.wulin.temp1.domain.properties.bean_post_processor.MyInstantiationAwareBeanPostProcessor">
	</bean>
	<bean id="apple" class="cn.wulin.temp1.domain.properties.bean_post_processor.Apple">
		<spring:property name="name">
			<spring:value>红心苹果</spring:value>
		</spring:property>
	</bean>
	     
</beans>